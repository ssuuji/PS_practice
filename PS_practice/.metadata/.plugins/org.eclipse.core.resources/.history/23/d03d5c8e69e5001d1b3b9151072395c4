package py2884;

import java.util.Scanner;

public class Main {//알람시계

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int h = sc.nextInt();
		int m = sc.nextInt();
		
		if(m < 45) {
			if(h==0) {
				h = 23-h;
			}else {
				h=h-1;
			}
			m = 60-(45-m);	
			
			System.out.println(h+" "+m);
		}
		else {
			m = m-45;
			System.out.println(h+" "+m);
		}
		
		
		
		

	}

}
